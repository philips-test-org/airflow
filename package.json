{
  "name": "airflow",
  "version": "1.2.0",
  "main": "index.js",
  "repository": "git@github.com:analytical-informatics/airflow.git",
  "author": "Travis Poulsen <travis.poulsen@philips.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "yarn eslint --color -c web/.eslintrc web --ignore-path web/.eslintignore",
    "lint:watch": "yarn esw -w -f simple-detail --clear --color -c web/.eslintrc web --ignore-path web/.eslintignore",
    "flow:watch": "flow-watch",
    "start": "webpack --watch --config webpack.dev.js",
    "build": "yarn webpack --config webpack.prod.js",
    "watch": "webpack --watch -d --hide-modules --progress --config webpack.dev.js --env.DEBUG=true",
    "watch:hot": "webpack-dev-server --progress --inline --hot --config webpack.dev.js --port 3000",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "__tests__/generators.js",
      "__tests__/helpers.js",
      "__tests__/setupTests.js",
      "__tests__/mockState/*"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/web/__tests__/setupTests.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "diff": "^3.4.0",
    "keymaster": "^1.6.2",
    "moment": "^2.20.1",
    "raf": "^3.4.0",
    "ramda": "^0.25.0",
    "react": "16.3.2",
    "react-dates": "^16.3.5",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "16.3.2",
    "react-redux": "^5.0.6",
    "react-toggle": "^4.0.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "url-search-params-polyfill": "^4.0.0",
    "uuid": "^3.2.1",
    "whatwg-fetch": "^2.0.4",
    "why-did-you-update": "^0.1.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-ramda": "^1.6.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.12.1",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.4",
    "fetch-mock": "^6.4.2",
    "flow-bin": "^0.71.0",
    "flow-watch": "^1.1.3",
    "jest": "^22.4.3",
    "jest-enzyme": "^6.0.0",
    "jsverify": "^0.8.3",
    "prettier": "1.10.2",
    "react-hot-loader": "^4.1.3",
    "react-test-renderer": "^16.3.2",
    "redux-devtools-extension": "^2.13.2",
    "style-loader": "^0.20.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  }
}
